{
  "name": "unifind",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/unifind",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "root": "apps/unifind",
        "outputPath": "dist/apps/unifind"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "unifind:build",
        "dev": true
      }
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sh deploy.sh prod apps/unifind"
      }
    }
  }
}
